{
    "name": "sqlite-admin/sqlite-admin",
    "description": "A simple SQLite Admin or SQLite viewer.",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\": "tests/"
      }
    },
    "authors": [
        {
            "name": "CodeWithSushil",
            "email": "sushilkumbhar77@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
      "php": "^8.0",
        "twbs/bootstrap-icons": "^1.11",
        "twbs/bootstrap": "^5.3"
    },
    "require-dev": {
      "phpstan/phpstan": "*",
        "pestphp/pest": "^2.34"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
